[access]
	inheritFrom = Public-Plugins
[project]
	description = A plugin which allows projects to be deleted from Gerrit via an SSH command
[access "refs/*"]
	owner = deny group google/gerritcodereview-maintainers@googlegroups.com
	owner = deny group plugins-delete-project
	submit = deny group plugins-delete-project
[receive]
[submit]
[access "refs/tags/*"]
	createSignedTag = deny group gerrit-release-managers
